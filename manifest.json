{
  "name": "Doggo Backgrounds",
  "description": "Presents you with a picture of Beasley or Levi everytime you open a tab",
  "version": "1.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "DogPerk.png"
    }
},
  "permissions": [
    "storage",
    "alarms"
  ]
}
